<div class="col-md-12" style="margin-bottom:20px;">    
    <h2>My Leads <span class="create-btn"><a href="/researcher/leads/add"><i class="fa fa-plus"></i></a></span></h2>
    
    <% if (flash.success) { %>
        <div class="alert alert-success" role="alert">
            <strong><%= flash.success %></strong>
        </div>
    <% } %>

    <% if (flash.error) { %>
        <div class="alert alert-warning" role="alert">
            <strong><%= flash.error %></strong>
        </div>
    <% } %>

    <% if(leads) {%>
    <table id="myListDataTable" class="display" style="width:100%">
        <thead>
            <tr>
                <th>Applicant Name</th>
                <th>Word Mark</th>
                <th>Serial Number</th>
                <th>Registration Number</th>
                <th>Office Status</th>
                <th>Lead Status</th>
                <th>Actions</th>
            </tr>
        </thead>
        
            <tbody>
            <% leads.forEach(function(lead,key) { %>
                <tr>
                    <td><%= lead.applicant_name %></td>
                    <td><%= lead.word_mark %></td>
                    <td><%= lead.trademark_number %></td>
                    <td><%= lead.trademark_registration_number %></td>
                    <td><%= lead.trademark_status %></td>
                    <td><%= lead.lead_status %></td>
                   
                    <td>
                        <a href="/researcher/leads/view/<%= lead._id %>"><i class="fa fa-eye"></i></a>
                        <a href="/researcher/leads/edit/<%= lead._id %>"><i class="fa fa-pen"></i></a>
                        <!-- <a href=""><i class="fa fa-trash"></i></a> -->
                    </td>
                </tr>
            <% }) %>
                    
            </tbody>
        
    </table>
    <% } else { %>
        <p>Sorry, No Lead(s) Found!</p>
    <% } %>
</div>
        