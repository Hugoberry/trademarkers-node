<div class="col-md-12" style="margin-bottom:20px;">  
    <h2>Tasks List </h2>

    <% if (flash.success) { %>
    <div class="alert alert-success" role="alert">
        <strong><%= flash.success %></strong>
    </div>
    <% } %>

    <% if (flash.error) { %>
    <div class="alert alert-danger" role="alert">
        <strong><%= flash.error %></strong>
    </div>
    <% } %>
</div>
                    
<div clss="col-md-12">
    <table id="myListDataTable" class="display" style="width:100%">
        <thead>
            <tr>
                <th>Mark</th>
                <th>Serial Number</th>
                <th>Registration Number</th>
                <th>Status</th>
                <th>Status date</th>
                <th>Action</th>
            </tr>
        </thead>
        <tbody>
        <% trademarks.forEach(function(trademark,key) { if(trademark.mark) { %>
   
            <tr>
                <td><%= trademark.mark %></td>
                <td><%= trademark.serialNumber %></td>
                <td><%= trademark.registrationNumber %></td>
                <td><%= trademark.status %></td>
                <td><%= trademark.statusDate %></td>
                <td><a href="/njs-admin/manage/trademark/edit/<%= trademark._id %>"><i class="fa fa-pen"></i></a></td>
            </tr>
            <% } }) %>
        </tbody>
    </table>
</div>