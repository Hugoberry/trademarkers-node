<div class="col-md-12" style="margin-bottom:20px;">    
    <h2>View Lead</h2>
        
        <div class="row">
            <div class="col-md-6">

            <div class="form-group">
                <label>Select Task Number (<%= lead.task_name %>)</label>
                <input type="text" class="form-control" value="<%= lead.task_number %>" placeholder="" name="word_mark" disabled>

            </div>

            

            <div class="form-group">
                <label>Word Mark</label>
                <input type="text" class="form-control" value="<%= lead.word_mark %>" name="word_mark" disabled>
            </div>

            <div class="form-group">
                <label>Trademark Serial Number</label>
                <input type="text" class="form-control" value="<%= lead.trademark_number %>" name="trademark_number" disabled>
            </div>

            <div class="form-group">
                <label>Trademark Registration Number</label>
                <input type="text" class="form-control" value="<%= lead.trademark_registration_number %>" name="trademark_registration_number" disabled>
            </div>

            <div class="form-group">
                <label>Trademark Registration Date</label>
                <input type="date" class="form-control" value="<%= lead.trademark_registration_date %>" name="trademark_registration_date" disabled>
            </div>

            <div class="form-group">
                <label>Trademark Application Date</label>
                <input type="date" class="form-control" value="<%= lead.trademark_application_date %>" name="trademark_application_date" disabled>
            </div>

            <div class="form-group">
                <label>Trademark Status</label>
                <input type="text" class="form-control" value="<%= lead.trademark_status %>" name="trademark_status" disabled>
            </div>

            <div class="form-group">
                <label>Trademark Status Date</label>
                <input type="date" class="form-control" value="<%= lead.trademark_status_date %>" name="trademark_status_date" disabled>
            </div>

            
            

        </div>

        <div class="col-md-6">

            <div class="form-group">
                <label>Lead Status</label>
                <input type="text" class="form-control" value="<%= lead.event_status %>" name="event_status" disabled>
            </div>

            <div class="form-group">
                <label>Applicant Name</label>
                <input type="text" class="form-control" value="<%= lead.applicant_name %>" name="applicant_name" disabled>
            </div>

            

            <div class="form-group">
                <label>Address</label>
                <input type="text" class="form-control" value="<%= lead.applicant_address %>" name="applicant_address" disabled>
            </div>

            <div class="form-group">
                <label>Postal Code</label>
                <input type="text" class="form-control" value="<%= lead.applicant_postal %>" name="applicant_postal" disabled>
            </div>

            <div class="form-group">
                <label>City</label>
                <input type="text" class="form-control" value="<%= lead.applicant_city %>" name="applicant_city" disabled>
            </div>

            <div class="form-group">
                <label>Country</label>
                <input type="text" class="form-control" value="<%= lead.applicant_country %>" name="applicant_country" disabled>
            </div>

            <div class="form-group">
                <label>Trademark Class(es)</label>
                <input type="text" class="form-control" value="<%= lead.trademark_class %>" name="trademark_class" disabled>
            </div>

            <div class="form-group">
                <label>Applicant Email</label>
                <input type="text" class="form-control" value="<%= lead.applicant_email %>" name="applicant_email" disabled>
            </div>
            
            


            

        </div>

        <div class="col-md-12">
            <p class="text-center">
                <a class="btn btn-primary btn-rounded-primary" href="/njs-admin/manage/leads/edit/<%= lead._id %>">Edit Lead</a>
            </p>
        </div>

    </div>


</div>
        