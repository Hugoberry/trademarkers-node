<!-- 2nd block -->

<div class="row d-none d-md-block d-lg-block d-xl-block">
	<div class="col-12 ">
		
		<div class="network-box-form">
			<div id="continent" class="box-form">
				<h5> I would like to register a trademark in : <span></span> </h5>
				<!-- <div class="box-search-rounded-select" aria-controls="continentsToggle">
					<span id="selectedContinent">Europe</span>
				</div> -->

			
				<ul id="continentsToggle" class="continentsToggle ">
				<% for (var key in continent) { %>
					<li class="<%= (continentCountries[key].name == 'Europe' ? 'active':'') %>" data-abbr="<%= continent[key].id %>" data-name="<%= continent[key].name %>"><%= continent[key].name %></li>
				<% } %>
				</ul>

			</div>

			<div class="country-contents">
				
			
				
				<% for (var key in continentCountries) { %>
				<div class="row continent continent-<%= continentCountries[key]['id'] %> <%= (continentCountries[key].name == 'Europe' ? 'show':'') %>" >
					<% for ( var ckey in continentCountries[key].countries ) { %>
					
					<div class="col-md-3">
						<a href="/trademark-registration-in-<%= continentCountries[key].countries[ckey].name.replace(' ', '_').toLowerCase() %>" >
							<img src="images/<%= continentCountries[key].countries[ckey].avatar %>"><%= continentCountries[key].countries[ckey].name %>
						</a>
					</div>
	
					<% } %>
				</div>
				<% } %>
				
	
					
					
				</div>

		</div>

		

	</div>
</div>


