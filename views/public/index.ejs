</div>


<!-- 2nd block -->

<div class="row d-none d-md-block d-lg-block d-xl-block">
	<div class="col-12 ">
		
		<div class="network-box-form">
			<div id="continent" class="box-form">
				<h5> I would like to register a trademark in : <span></span> </h5>
				
				<ul id="continentsToggle" class="continentsToggle ">
				<% for (var key in continents) { %>
					<% if(continents[key].countries.length > 0) { %>
					<li class="<%= (continents[key].name == 'Europe' ? 'active':'') %>" data-abbr="<%= continents[key].id %>" data-name="<%= continents[key].name %>"><%= continents[key].name %></li>
					<% } %>
				<% } %>
				</ul>

			</div>

			<div class="country-contents">
				
				<% for (var key in continents) {  %>
				<div class="row continent continent-<%= continents[key]['id'] %> <%= (continents[key].name == 'Europe' ? 'show':'') %>" >
					<% for ( var ckey in continents[key].countries ) { if( continents[key].countries[ckey].name && continents[key].countries[ckey].status != "disabled" ) { %>

					<div class="col-md-3">
						<a href="/trademark-registration-in-<%= continents[key].countries[ckey].name.replace(/\s/g, '_').toLowerCase() %>" >
							<% if( continents[key].countries[ckey].avatar && continents[key].countries[ckey].avatar != "Test") { %><img src="images/<%= continents[key].countries[ckey].avatar %>"><% } %><%= continents[key].countries[ckey].name %>
						</a>
					</div>
	
					<% } } %>
				</div>
				<% }  %>
				
			</div>

		</div>

	</div>
</div>


