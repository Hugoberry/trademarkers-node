  <script>
$( document ).ready(function() {
  $(".frm-sou").submit(function(){
    
    let action = $(this).attr('data-action');
    let number = $(this).attr('data-action-number');
    let response = $(this).attr('data-respond');
    let name = $("#decName").val()
    // let redUrl = 'pay';

    $.ajax({
      url: "/action/response/"+action+"/"+response,
      type:"GET",
      dataType:"json",
      data: {
        action: action,
        response: response,
        decName: name
      },
      contentType: "application/json",
      success: function( result ) {

        if ( (response * 1) < 3) { 
          $(location).attr('href', '/'+ number +'/pay')
        } else { 
          $(location).attr('href', '/')
        }
        
      }
    }); 

      return false;
  });

  var shipmentOptInput = $('.crt-fct-opt > .fieldset > input[type="radio"]');
  // var fedexShipmentOptInput = $('.crt-fct-opt-fedex > .fieldset > input[type="checkbox"]');

  $(shipmentOptInput).change(function(){
    if(this.checked != true){
      $(this).parent('.fieldset').parent('.crt-fct-opt').removeClass('crt-fct-opt-active');
      return false;
    }
    else{
      $(this).parent('.fieldset').parent('.crt-fct-opt').addClass('crt-fct-opt-active');
        return true;
    }
  });

  $('#CertificateDeliverySelection').click(function(){
    if(shipmentOptInput.prop("checked") == true){
      // SELECTED FEDEX
      $('.cc-section-on-call').removeClass("cc-section-on-call-active");
      $('html, body').animate({scrollTop: "0"}, 1000);
      return false;

